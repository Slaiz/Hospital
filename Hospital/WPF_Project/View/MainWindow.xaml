<Window x:Class="WPF_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="1000">
    <Grid>
        <StackPanel>
            <Button Content="Patient Table" Height="40" Width="75" Click="PButton_Click" VerticalAlignment="Center" HorizontalAlignment="Left" />
            <Button Content="Doctor Table" Height="40" Width="75" Click="DButton_Click" />
            <Button Content="Examination Table" Height="40" Width="75" Click="EButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
